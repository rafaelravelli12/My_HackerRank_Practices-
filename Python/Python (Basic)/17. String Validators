text = input()

text_list = list(text)
for i in range(len(text_list)):
    text_list[i] = text_list[i].isalnum()
b = 0
for i in range(len(text_list)):
    if text_list[i] == True:
        b += 1
    else:
        b += 0
if b > 0:
    print(True)
else:
    print(False)
    
text_list = list(text)
for i in range(len(text_list)):
    text_list[i] = text_list[i].isalpha()
b = 0
for i in range(len(text_list)):
    if text_list[i] == True:
        b += 1
    else:
        b += 0
if b > 0:
    print(True)
else:
    print(False)
    
text_list = list(text)
for i in range(len(text_list)):
    text_list[i] = text_list[i].isdigit()
b = 0
for i in range(len(text_list)):
    if text_list[i] == True:
        b += 1
    else:
        b += 0
if b > 0:
    print(True)
else:
    print(False)
    
text_list = list(text)
for i in range(len(text_list)):
    text_list[i] = text_list[i].islower()
b = 0
for i in range(len(text_list)):
    if text_list[i] == True:
        b += 1
    else:
        b += 0
if b > 0:
    print(True)
else:
    print(False)
    
text_list = list(text)
for i in range(len(text_list)):
    text_list[i] = text_list[i].isupper()
b = 0
for i in range(len(text_list)):
    if text_list[i] == True:
        b += 1
    else:
        b += 0
if b > 0:
    print(True)
else:
    print(False)
    
